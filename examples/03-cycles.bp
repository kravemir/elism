fn main() -> i32
{
    for(i: range(0,10)) {
        if( i < 5 )
            printf("i: %d < 5\n", i);
        else
            printf("i: %d >= 5\n", i);
    }

    var i2 = 1;
    var e_22n = 2;
    while(i2 <= 16) {
        printf("i2: %d, e_22n: %d\n", i2, e_22n);
        i2 = i2 * 2;
        e_22n = e_22n * e_22n;
    }

    return 0;
}